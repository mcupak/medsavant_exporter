{
    "name": "medsavant_exporter",
    "title": "Export to MedSavant",
    "summary": "Exporter of variant data to MedSavant",
    "description": "This app exports VCF data into a given MedSavant server.",
    "dxapi": "1.0.0",
    "inputSpec": [
        {
            "optional": false,
            "name": "host",
            "class": "string"
        },
        {
            "optional": false,
            "name": "port",
            "class": "int"
        },
        {
            "optional": false,
            "name": "db",
            "class": "string"
        },
        {
            "optional": false,
            "name": "project",
            "class": "int"
        },
        {
            "optional": false,
            "name": "refId",
            "class": "int"
        },
        {
            "optional": false,
            "name": "includeHomoRef",
            "class": "boolean"
        },
        {
            "optional": false,
            "name": "autoPublish",
            "class": "boolean"
        },
        {
            "optional": false,
            "name": "preAnnotateWithJannovar",
            "class": "boolean"
        },
        {
            "optional": false,
            "name": "email",
            "class": "string"
        },
        {
            "optional": false,
            "name": "username",
            "class": "string"
        },
        {
            "optional": false,
            "name": "password",
            "class": "string"
        }
    ],
    "outputSpec": [
        {
            "name": "status",
            "class": "string"
        }
    ],
    "runSpec": {
        "code": "export CLASSPATH=${DX_FS_ROOT}/:${DX_FS_ROOT}/opt/jar/medsavant-shared-1.3.1.jar:${DX_FS_ROOT}/opt/jar/common-util-1.0.4.jar:$CLASSPATH; java org.ut.biolab.medsavant.MedSavantExporter",
        "interpreter": "bash",
        "execDepends": [
            {
                "name": "dx-java-bindings"
            },
            {
                "name": "openjdk-6-jre-headless"
            }
        ]
    },
    "access": {
        "network": ["*"]
    },
    "version": "0.0.1"
}
